<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.labs.android.lab7.Fragments.ActivityForFragments">

    <fragment
        android:id="@+id/fragment"
        android:name="com.labs.android.lab7.Fragments.FragmentTimeDemo"
        android:layout_width="320dp"
        android:layout_height="79dp"
        tools:layout="@layout/fragment_time_demo"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="348dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.894"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <fragment
        android:id="@+id/fragment2"
        android:name="com.labs.android.lab7.Fragments.FragmentProductList"
        android:layout_width="169dp"
        android:layout_height="151dp"
        tools:layout="@layout/fragment_fragment_product_list"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginStart="16dp"
        android:layout_weight="1"
        android:layout_marginEnd="8dp" />

    <fragment
        android:id="@+id/fragment3"
        android:name="com.labs.android.lab7.Fragments.FragmentDetail"
        android:layout_width="180dp"
        android:layout_height="152dp"
        tools:layout="@layout/fragment_fragment_detail"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="25dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.957"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"/>

</android.support.constraint.ConstraintLayout>
